/* EventBrite import event page */
section.iee_content_body{
	display:flex;
	flex-direction:column;
	margin-top:-12em;
} 

.iee_content_body h3{
	margin-top:0;
}

.iee_content_body #hero{
	order:1;
}
.iee_content_body #event_details{
	order:1;
	display:flex;
	flex-direction: column;
}
@media only screen and (min-width: 800px) {
  .iee_content_body #event_details{
    flex-direction: row;
  }
	.iee_content_body h3{
	margin-top:2em;
}
}




.iee_content_body #event_details #cost #dollar_amount{
	display:block;
	font-size: 2em;
	margin-bottom: 0.3em;
}



.iee_content_body .iee_event_meta{
	order:2;
}

.iee_content_body .eventbrite-ticket-section{
	order:3;
}
.iee_content_body #images_from_class{
	order:4;
	
}

.iee_content_body #testimonial{
	order:5;	
	margin:auto;
}
.iee_content_body .faq{
	order:6;
}
.iee_content_body #event_details ul{
	padding:0;
	margin-top:-1.25em;
}

.iee_content_body #event_details > *{
	margin-right: 1.5em;
}

.iee_content_body #event_details #description{
	flex-grow:2;
}

.iee_content_body #event_details #includes, #event_details #cost{
	flex-grow:1;
}

.iee_content_body .venue .titlemain{
	font-size:1.68em;
	font-family:'Lato', sans-serif;
	font-weight:300;
	line-height:1.3em;
	color:#888;
	margin-bottom:0.5em;
}

.iee_content_body #images_from_class #thumbnails{
	display:flex;
	flex-wrap:wrap;
	justify-content:center;
}




.iee_content_body #images_from_class #thumbnails img{
	width:150px;
	height:150px;
	padding:.5em 0;
}

.iee_content_body .iee_event_meta .iee_organizermain{
	padding:0;
	padding-top:3em;
	
}
.iee_content_body .iee_event_meta .iee_organizermain:nth-child(1){
	display:none;
}

.iee_content_body #testimonial p{

	font:1.3em/1.4em "Old Standard TT", Georgia, "Times New Roman", Times, serif;
	font-style:italic;
}

.faq h4{
	color:#ccc;
	font-size:1.3em;
	margin-bottom:-0.2em;
}

/* Home Page EventBrite import */
.home_module .iee_archive .iee_event .event_details{
	background-color: #000;
	color: #888;
	line-height:1.8em;
	font-size: 0.9em;
	border-bottom:1px solid #222;
}



.home_module .iee_archive .iee_event .event_desc .event_title{
	color: #b00;
	font-size:1.3em
}
.home_module .iee_archive .iee_event .event_date {
	background-color: #222;
	width:55px;
	padding-top:1em;
	
}
.home_module .iee_archive .iee_event .event_date .month{
	font-size:0.8em;
	text-transform:uppercase;
	letter-spacing:0.1em;
}
.home_module .iee_archive .iee_event .event_date .date{
	font-size:1.5em;
}
.home_module .iee_archive nav{
	display:none;
}

/* calendar styles */
.iee_widget_style1 div.event_details{
	background:none;
	margin-top:1em;
	margin-bottom:2em;
}

.iee_widget_style1 div.event_desc{
	width:auto;
	padding:10px;
	padding-left:20px;
}

.iee_widget_style1 div.event_date{
	background: #a00;
}

.iee_widget_style1 .event_date span.month{
	font-size: 0.8em;
	text-transform: uppercase;
	letter-spacing:.2em;
	padding-top:.6em;
}

.iee_widget_style1 .event_date span.date{
	line-height:1.3em;
	padding-bottom: .4em;
}

/* event details */

.iee_event_meta div.iee_organizermain{
	background:none;
	border: none
}
/* home eventbrite eventtitle */
div.home_module div.eventbrite-item-title-col h3.eventbrite-item-title {
